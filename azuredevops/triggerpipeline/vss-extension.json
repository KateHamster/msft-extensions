{
  "manifestVersion": 1,
  "id": "maikvandergaag-trigger-pipeline",
  "name": "Trigger Azure DevOps Pipeline",
  "version": "3.0.0",
  "publisher": "maikvandergaag",
  "description": "Build and Release Management task to trigger a Azure DevOps release or build pipeline.",
  "public": true,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "images/icon.png"
  },
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "Release",
    "Build",
    "Azure DevOps",
    "Trigger",
    "3fifty"
  ],
  "screenshots": [
    {
        "path": "images/screenshot-1.png"
    },
    {
        "path": "images/screenshot-2.png"
    }
  ],
  "content": {
    "license": {
      "path": "license.md"
    },
    "details": {
      "path": "readme.md"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/MaikvanderGaag/msft-extensions/wiki"
    },
    "repository": {
      "uri": "https://github.com/MaikvanderGaag/msft-extensions/"
    },
    "issues": {
      "uri": "https://github.com/MaikvanderGaag/msft-extensions/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/maikvandergaag/msft-extensions/tree/master/azuredevops/triggerpipeline"
  },
  "branding": {
    "color": "rgb(0, 171, 236)",
    "theme": "light"
  },
  "contributions": [
    {
      "id": "TriggerPipeline",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "TriggerPipeline"
      }
    },
    {
      "id": "azuredevops-endpoint-type",
      "description": "Azure DevOps Service Endpoint",
      "type": "ms.vss-endpoint.service-endpoint-type",
      "targets": [ "ms.vss-endpoint.endpoint-types" ],
      "properties": {
          "name": "AzureDevOpsOrganization",
          "displayName": "Azure DevOps Organization",
          "url": {
            "displayName": "Azure DevOps Url",
            "value": "https://dev.azure.com/",
            "isVisible": "false"
          },
          "inputDescriptors": [
            {
                "id": "OrganizationName",
                "name": "Azure DevOps Organization",
                "description": "The name of the Azure DevOps Organization",
                "inputMode": "textbox",
                "isConfidential": false,
                "validation": {
                    "isRequired": true,
                    "dataType": "string",
                    "maxLength": 300
                }
            }
        ],
        "authenticationSchemes": [
            {
                "type": "ms.vss-endpoint.endpoint-auth-scheme-basic",
                "displayName": "Credentials",
                "inputDescriptors": [
                    {
                        "id": "username",
                        "name": "Username",
                        "description": "Specify the account of the user used for the actions.",
                        "inputMode": "textbox",
                        "isConfidential": false,
                        "validation": {
                            "isRequired": true,
                            "dataType": "string",
                            "maxLength": 300
                        }
                    },
                    {
                      "id": "password",
                      "name": "Personal Access Token",
                      "description": "",
                      "inputMode": "textbox",
                      "isConfidential": true,
                      "validation": {
                          "isRequired": true,
                          "dataType": "string",
                          "maxLength": 300
                      }
                  }
                ]
            }
        ],
          "helpMarkDown": "",
          "icon": "images/connection-icon.png"
      }
  }
  ],
  "files": [
    {
      "path": "triggerpipeline"
    }
  ]
}